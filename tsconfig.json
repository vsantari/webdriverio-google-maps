{
    "compilerOptions": {
        "target": "ES2015",
        "module": "commonjs",
        "baseUrl": ".",
        "paths": {
            "*": [ "./*" ],
            "src/*": ["./src/*"]
        },
        "types": ["node", "@wdio/sync", "@wdio/mocha-framework", "@wdio/devtools-service"],
        "experimentalDecorators": true, 
        "emitDecoratorMetadata": true, 
        
    },
    "include": [
        "./src/**/*.ts"
    ],
    "exclude": [
        "src/concepts"
    ]
}
